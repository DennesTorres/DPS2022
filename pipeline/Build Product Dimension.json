{
	"name": "Build Product Dimension",
	"properties": {
		"activities": [
			{
				"name": "Get Current Version",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "sqlSource",
					"type": "LinkedServiceReference",
					"parameters": {
						"servername": {
							"value": "@pipeline().globalParameters.servername",
							"type": "Expression"
						},
						"databasename": {
							"value": "@pipeline().globalParameters.databasename",
							"type": "Expression"
						},
						"username": {
							"value": "@pipeline().globalParameters.username",
							"type": "Expression"
						},
						"secretname": {
							"value": "@pipeline().globalParameters.passwordkey",
							"type": "Expression"
						}
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"parameters": [
								{
									"name": "LastVersion",
									"type": "Int64",
									"value": null,
									"direction": "Output"
								}
							],
							"type": "Query",
							"text": "SELECT @LastVersion = CHANGE_TRACKING_CURRENT_VERSION()"
						}
					]
				}
			},
			{
				"name": "Get Latest Version",
				"type": "Script",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "sqlSource",
					"type": "LinkedServiceReference",
					"parameters": {
						"servername": {
							"value": "@pipeline().globalParameters.servername",
							"type": "Expression"
						},
						"databasename": {
							"value": "@pipeline().globalParameters.dwdatabase",
							"type": "Expression"
						},
						"username": {
							"value": "@pipeline().globalParameters.username",
							"type": "Expression"
						},
						"secretname": {
							"value": "@pipeline().globalParameters.passwordkey",
							"type": "Expression"
						}
					}
				},
				"typeProperties": {
					"scripts": [
						{
							"parameters": [
								{
									"name": "LastVersion",
									"type": "Int64",
									"value": null,
									"direction": "Output"
								}
							],
							"type": "Query",
							"text": "select @LastVersion=LastVersion from dbo.SCDControl\nwhere tablename='Product'"
						}
					]
				}
			}
		],
		"folder": {
			"name": "ProductDimension"
		},
		"annotations": []
	}
}